        <!-- - - AC-2 (5) - -  -->
        <!-- - - AC-2 (12) - -  -->
        <!-- - - AC-12 - -  -->
        <!-- - ALTER CONTROLS - -->
        <!-- - - AC-2 - -  -->
        <!-- - - AC-2 (5) - -  -->
        <alter control-id="ac-2.5">
            <add position="ending" id-ref="ac-2.5_smt">
                <part id="ac-2.5_smp" name="item" ns="sample">
                    <title>AC-2 (5) Additional Sample Profile Requirements and Guidance</title>
                    <part id="ac-2.5_smp_gdn.1" name="guidance">
                        <prop name="label">Supplemental Guidance:</prop>
                        <p>The control enhancement should be implemented for all priviliged accountsThe organization-defined time-period of inactivity should be shorter 
                            than the session termination or disconnect set by AC-12</p>
                    </part>
                </part>
            </add>
        </alter>
        <!-- - - AC-2 (12) - -  -->
