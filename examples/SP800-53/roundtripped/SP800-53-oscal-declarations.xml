<declarations xmlns="http://csrc.nist.gov/ns/oscal/1.0">
  <declare-prop class="name" context="SP800-53">
    <singleton>true</singleton>
    <required>true</required>
    <regex>^(AC|AT|AU|CA|CM|CP|IA|IR|MA|MP|PE|PL|PM|PS|RA|SA|SC|SI)[\d\.\-]*$</regex>
  </declare-prop>
  <declare-prop class="priority" context="SP800-53">
    <singleton>true</singleton>
    <value>P1</value>
    <value>P2</value>
    <value>P0</value>
    <value>P3</value>
  </declare-prop>
  <declare-prop class="baseline-impact" context="SP800-53">
    <value>LOW</value>
    <value>MODERATE</value>
    <value>HIGH</value>
  </declare-prop>
  <declare-prop class="name" context="SP800-53-enhancement">
    <singleton>true</singleton>
    <required>true</required>
    <calc>
      <inherit></inherit>
      <autonum>(1)</autonum> 
    </calc>
  </declare-prop>
  <declare-prop class="baseline-impact" context="SP800-53-enhancement">
    <value>LOW</value>
    <value>MODERATE</value>
    <value>HIGH</value>
  </declare-prop>
  <declare-prop class="name" context="item">
    <singleton>true</singleton>
    <required>true</required>
    <calc>
      <inherit></inherit>
      <autonum>a.</autonum>
    </calc>
    <calc>
      <inherit></inherit>
      <autonum>(a)</autonum>
    </calc>
    <calc>
      <inherit></inherit>
      <autonum>1.</autonum>
    </calc>
    <calc>
      <inherit></inherit>
      <autonum>(1)</autonum>
    </calc>
  </declare-prop>
  <declare-prop class="description" context="item">
    <singleton>true</singleton>
    <required>true</required>
  </declare-prop>
  <declare-prop class="name" context="objective">
    <singleton>true</singleton>
  </declare-prop>
  <declare-prop class="method" context="assessment">
    <singleton>true</singleton>
    <required>true</required>
    <value>EXAMINE</value>
    <value>INTERVIEW</value>
    <value>TEST</value>
  </declare-prop>
  <declare-part class="statement" context="SP800-53">
    <singleton>true</singleton>
    <required>true</required>
  </declare-part>
  <declare-part class="guidance" context="SP800-53">
    <singleton>true</singleton>
  </declare-part>
  <declare-part class="objective" context="SP800-53">
    <singleton>true</singleton>
    <required>true</required>
  </declare-part>
  <declare-part class="assessment" context="SP800-53"></declare-part>
  <declare-part class="statement" context="SP800-53-enhancement">
    <singleton>true</singleton>
    <required>true</required>
  </declare-part>
  <declare-part class="guidance" context="SP800-53-enhancement">
    <singleton>true</singleton>
  </declare-part>
  <declare-part class="objective" context="SP800-53-enhancement">
    <singleton>true</singleton>
    <required>true</required>
  </declare-part>
  <declare-part class="assessment" context="SP800-53-enhancement"></declare-part>
  <declare-part class="description" context="statement">
    <singleton>true</singleton>
    <required>true</required>
  </declare-part>
  <declare-part class="item" context="statement"></declare-part>
  <declare-part class="item" context="item"></declare-part>
  <declare-part class="objective" context="objective">
    <required>true</required>
  </declare-part>
  <declare-p class="decision" context="objective">
